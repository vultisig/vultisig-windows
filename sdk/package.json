{
  "name": "@vultisig/sdk",
  "version": "1.0.0",
  "description": "VultiSig SDK for React/Vite/JS applications",
  "main": "dist/index.js",
  "module": "dist/index.mjs",
  "types": "dist/index.d.ts",
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.mjs",
      "require": "./dist/index.js"
    },
    "./ui": {
      "types": "./dist/ui.d.ts",
      "import": "./dist/ui.mjs",
      "require": "./dist/ui.js"
    }
  },
  "files": [
    "dist",
    "README.md"
  ],
  "scripts": {
    "build": "tsc",
    "typecheck": "tsc --noEmit",
    "dev": "tsc --watch"
  },
  "peerDependencies": {
    "react": "^18.0.0",
    "react-dom": "^18.0.0"
  },
  "peerDependenciesMeta": {
    "react": {
      "optional": true
    },
    "react-dom": {
      "optional": true
    }
  },
  "dependencies": {
    "@bufbuild/protobuf": "^2.2.4",
    "@core/chain": "workspace:*",
    "@core/config": "workspace:*",
    "@core/mpc": "workspace:*",
    "@core/ui": "workspace:*",
    "@lib/dkls": "workspace:*",
    "@lib/schnorr": "workspace:*",
    "@lib/ui": "workspace:*",
    "@lib/utils": "workspace:*",
    "axios": "^1.8.3"
  },
  "devDependencies": {
    "@vitejs/plugin-react": "^4.3.4",
    "buffer": "^6.0.3",
    "crypto-browserify": "^3.12.0",
    "stream-browserify": "^3.0.0",
    "typescript": "^5.8.2",
    "vite": "^6.2.2",
    "vite-plugin-dts": "^4.4.0",
    "vite-plugin-node-polyfills": "^0.24.0",
    "vite-plugin-wasm": "^3.4.1"
  }
}
